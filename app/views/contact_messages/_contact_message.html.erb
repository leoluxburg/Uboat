<div id="<%= dom_id contact_message %>">
  <p>
    <strong>Name:</strong>
    <%= contact_message.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact_message.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= contact_message.phone %>
  </p>

  <p>
    <strong>Message:</strong>
    <%= contact_message.message %>
  </p>

</div>
